<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Community</title>
</head>
<body>
  <%- include('../partials/_navbar.ejs') %>
  <h1>Explore Our Community</h1>

  <% if (users && users.length > 0) { %>
    <ul>
      <% users.forEach(function(u) { %>
        <li>
          <a href="/users/<%= u._id %>"><%= u.username %></a>
        </li>
      <% }); %>
    </ul>
  <% } else { %>
    <p>No users found.</p>
  <% } %>
</body>
</html>
