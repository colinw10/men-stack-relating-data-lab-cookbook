<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title><%= otherUser.username %>'s Pantry</title>
</head>
<body>
  <%- include('../partials/_navbar.ejs') %>
  <h1><%= otherUser.username %>'s Pantry</h1>

  <% if (otherUser.pantry && otherUser.pantry.length > 0) { %>
    <ul>
      <% otherUser.pantry.forEach(function(item) { %>
        <li><%= item.name %></li>
      <% }); %>
    </ul>
  <% } else { %>
    <p>This pantry is empty.</p>
  <% } %>
</body>
</html>
